   "source": [
    "%%writefile GetQuotientRemainder.py\n",
    "\n",
    "n1 = int(input(\"값 1을 입력하세요 : \"))\n",
    "n2 = int(input(\"값 2을 입력하세요 : \"))\n",
    "\n",
    "def getQuotientRemainder(a, b):\n",
    "    return a // b, a % b\n",
    "\n",
    "Q, R = getQuotientRemainder(n1, n2)\n",
    "print(\"몫 : %d, 나머지 : %d\" % (Q, R))"
   ]
